$TTL	86400
$ORIGIN 3.0.10.in-addr.arpa.

@	IN	SOA	week3.  root.week3. (
			      1		; Serial
			 604800		; Refresh
			  86400		; Retry
			2419200		; Expire
			  86400 )	; Negative Cache TTL

@	IN	NS	dns.week3.
1	IN	PTR	dns.week3.
2	IN	PTR	you.week3.
5	IN	PTR	target1.week3.
6	IN	PTR	target2.week3.
7	IN	PTR	target3.week3.
